{
  "private": true,
  "name": "sml-workspace",
  "version": "0.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "ts-check": "tsc --project ./packages/sml/tsconfig.json --noEmit",
    "build": "tsc --project ./packages/sml/tsconfig.json",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "^18.6.4",
    "typescript": "^4.8.3",
    "prettier": "latest",
    "vite": "^3.1.0",
    "vitest": "^0.23.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.9.3"
}
